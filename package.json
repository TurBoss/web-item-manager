{
  "name": "webapp",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "server": "node server",
    "server:dev": "cross-env NODE_ENV=development node server",
    "server:test": "cross-env NODE_ENV=test mocha server/tests/**/*",
    "s:test": "node ./scripts/server-test.js",
    "build": "webpack",
    "dev": "webpack-dev-server --content-base public",
    "client:test": "jest --no-cache",
    "test": "node ./scripts/server-test.js",
    "lint": "eslint **/*.js{,x} && npm run lint:style",
    "lint:style": "stylelint --syntax scss src/**/*.scss; exit 0"
  },
  "author": "Ross Weir",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-runtime": "^6.25.0",
    "bcrypt": "^1.0.2",
    "better-sqlite3": "^3.3.0",
    "body-parser": "^1.17.2",
    "bootstrap": "4.0.0-alpha.6",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "fs-extra": "^4.0.1",
    "jsonwebtoken": "^7.4.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-transition-group": "^2.2.0",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sqlite3": "^3.1.9"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.scss$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(js|jsx)$"
  }
}
